{
    "version": 1,
    "kind": "project",
    "id": "LibraryProjectTravis",
    "category": "H.Project",
    "trDescription": "Creates a library project",
    "trDisplayName": "Library Project",
    "trDisplayCategory": "Other Project",
    "icon": "../consoleapplication.png",
    "enabled": "%{JS: [ %{Plugins} ].indexOf('QmlProjectManager') >= 0}",
    "options": [
        {
            "key": "ProFileName",
            "value": "%{JS: Util.fileName('%{ProjectDirectory}/%{ProjectName}', 'pro')}"
        },
        {
            "key": "MainQmlFileName",
            "value": "%{JS: Util.fileName('%{ProjectName}', 'qml')}"
        },
        {
            "key": "QtQuickVersion",
            "value": "%{JS: %{QtVersion}.qtQuickVersion}"
        },
        {
            "key": "QtQuickWindowVersion",
            "value": "%{JS: %{QtVersion}.qtQuickWindowVersion}"
        },
        {
            "key": "UiSupport",
            "value": "%{JS: '%{QtQuickVersion}' !== '2.3' }"
        },
        {
            "key": "IsUiFileInUse",
            "value": "%{JS: %{UiSupport} && %{QmlUiSplit} }"
        }
    ],
    "pages": [
        {
            "trDisplayName": "Project Location",
            "trShortTitle": "Location",
            "typeId": "Project"
        },
        {
            "trDisplayName": "Project Management",
            "trShortTitle": "Summary",
            "typeId": "Summary"
        }
    ],
    "generators": [
        {
            "typeId": "File",
            "data": [
                {
                    "source": "LICENSE",
                    "target": "LICENSE"
                },
                {
                    "source": "MYPROJECT.pri",
                    "target": "MYPROJECT.pri"
                },
                {
                    "source": "qpm.json",
                    "target": "qpm.json"
                },
                {
                    "source": "README.md",
                    "target": "README.md"
                },
                {
                    "source": "tests/MYPROJECTunittests/.gitignore",
                    "target": "tests/MYPROJECTunittests/.gitignore"
                },
                {
                    "source": "tests/MYPROJECTunittests/main.cpp",
                    "target": "tests/MYPROJECTunittests/main.cpp"
                },
                {
                    "source": "tests/MYPROJECTunittests/MYPROJECTunittests.pro",
                    "target": "tests/MYPROJECTunittests/MYPROJECTunittests.pro"
                },
                {
                    "source": "tests/MYPROJECTunittests/qmltests/tst_QmlTests.qml",
                    "target": "tests/MYPROJECTunittests/qmltests/tst_QmlTests.qml"
                },
                {
                    "source": "tests/MYPROJECTunittests/qpm.json",
                    "target": "tests/MYPROJECTunittests/qpm.json"
                },
                {
                    "source": "tests/MYPROJECTunittests/tests.cpp",
                    "target": "tests/MYPROJECTunittests/tests.cpp"
                },
                {
                    "source": "tests/MYPROJECTunittests/tests.h",
                    "target": "tests/MYPROJECTunittests/tests.h"
                }
            ]
        }
    ]
}